package br.com.universidade.modelo;

public class Medio {

	private int tipo;

	
	public String getAll() {
		return
				"Tipo: " + tipo;
	}
	
	public void setAll(int tipo) {
		this.tipo = tipo;
	}
	
	public Medio() {
		super();
	}

	public Medio(int tipo) {
		super();
		this.tipo = tipo;
	}

	public int getTipo() {
		return tipo;
	}

	public void setTipo(int tipo) {
		this.tipo = tipo;
	}
	
	
}
