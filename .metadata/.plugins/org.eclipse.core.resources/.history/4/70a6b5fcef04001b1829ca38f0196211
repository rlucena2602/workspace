import br.com.universidade.tela.Magica;

public class TesteTurma {

	public static void main(String[] args) {
		
		Turma objeto = new Turma(
				Magica.s("Nome"),
				Magica.s("Periodo"),
				new Curso(
						new Formacao(
								Magica.f("Valor"),
								Magica.s("Descrição"),
								Magica.i("Duração")
								),
						Magica.s("Sigla"),
						Magica.s("Data de Lançamento")
						),
				new Aluno(),
				new Professor()
				);

	}

}
